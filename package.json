{
    "name": "@nordcom/ferrariclubsweden",
    "version": "0.1.0",
    "private": true,
    "prettier": "@nordcom/prettier",
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18.12"
    },
    "cacheDirectories": [
        ".next/cache"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "postbuild": "npm run sitemap",
        "sitemap": "next-sitemap",
        "postsitemap": "for i in public/*.xml; do cat $i | pretty-xml | tee $i > /dev/null; done",
        "start": "next start -p $PORT",
        "lint": "npm-run-all --parallel lint:**",
        "lint:next": "next lint",
        "lint:prettier": "prettier src/**/*.* --check",
        "lint:types": "tsc --project tsconfig.json -noEmit",
        "test": "npm-run-all --parallel test:**",
        "test:jest": "jest --coverage --passWithNoTests",
        "ci": "npm-run-all --parallel lint test",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/ferrariclubsweden.se.git"
    },
    "author": {
        "name": "Nordcom Group Inc.",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam SandstrÃ¶m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "bugs": {
        "url": "https://github.com/NordcomInc/ferrariclubsweden.se/issues"
    },
    "homepage": "https://www.ferrariclubsweden.se/",
    "browserslist": [
        ">0.3%",
        "defaults",
        "supports es6-module",
        "supports css-grid"
    ],
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@types/node": "20.8.3",
        "@types/react": "18.2.25",
        "eslint": "8.51.0",
        "eslint-config-next": "13.5.4",
        "eslint-config-prettier": "9.0.0",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "lint-staged": "14.0.1",
        "next-sitemap": "4.2.3",
        "npm-run-all": "4.1.5",
        "prettier": "3.0.3",
        "pretty-xml": "1.2.1"
    },
    "dependencies": {
        "@apollo/client": "3.8.5",
        "cheerio": "1.0.0-rc.12",
        "destyle.css": "4.0.0",
        "graphql": "16.8.1",
        "js-bbcode-parser": "4.0.0",
        "next": "13.5.4",
        "next-auth": "4.23.2",
        "next-seo": "6.1.0",
        "nprogress": "0.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "4.11.0",
        "react-responsive-carousel": "3.2.23",
        "styled-components": "6.0.8",
        "swr": "2.2.4",
        "typescript": "5.2.2"
    }
